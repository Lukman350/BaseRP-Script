{
  "user": "Lukman350",
  "repo": "BaseRP-Script",
  "entry": "gamemodes\\gm.pwn",
  "output": "gamemodes\\gm.amx",
  "local": true,
  "dependencies": [
    "sampctl/samp-stdlib",
    "pBlueG/SA-MP-MySQL:R41-4",
    "maddinat0r/sscanf:v2.8.3",
    "samp-incognito/samp-streamer-plugin:v2.9.4",
    "pawn-lang/YSI-Includes:4.x",
    "Zeex/samp-plugin-crashdetect:v4.19.4",
    "Southclaws/pawn-chrono:1.1.1"
  ],
  "builds": [
    {
      "name": "main",
      "args": ["-d3", "-;+", "-(+", "-Z+"],
      "includes": ["includes"]
    }
  ],
  "include_path": "includes",
  "runtime": {
    "version": "0.3.DL",
    "mode": "server",
    "echo": "Loading configuration...",
    "rcon_password": "password",
    "port": 7777,
    "hostname": "Base Roleplay Script - By Lukman",
    "gamemodes": ["gm"],
    "maxplayers": 50,
    "language": "Bahasa Indonesia / English",
    "mapname": "San Andreas",
    "weburl": "github.com/Lukman350",
    "gamemodetext": "Unknown",
    "plugins": ["pBlueG/SA-MP-MySQL:R41-4", "Zeex/samp-plugin-crashdetect:v4.19.4", "maddinat0r/sscanf:v2.8.3", "samp-incognito/samp-streamer-plugin:v2.9.4", "Southclaws/pawn-chrono:1.1.1"],
    "announce": true,
    "lanmode": false,
    "query": true,
    "rcon": false,
    "logqueries": false,
    "sleep": 5,
    "maxnpc": 0,
    "stream_rate": 1000,
    "stream_distance": 200,
    "onfoot_rate": 30,
    "incar_rate": 30,
    "weapon_rate": 30,
    "chatlogging": true,
    "timestamp": true,
    "messageholelimit": 3000,
    "messageslimit": 500,
    "ackslimit": 3000,
    "playertimeout": 10000,
    "minconnectiontime": 0,
    "lagcompmode": 1,
    "connseedtime": 300000,
    "db_logging": false,
    "db_log_queries": false,
    "conncookies": true,
    "cookielogging": false,
    "output": true
  }
}
